<div class="page-header">
  <%= link_to press_releases_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_press_release_path(@press_release), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show press_release</h1>
</div>

<dl class="dl-horizontal">
  <dt>Media type:</dt>
  <dd><%= @press_release.media_type %></dd>

  <dt>Media name:</dt>
  <dd><%= @press_release.media_name %></dd>

  <dt>Url:</dt>
  <dd><%= @press_release.url %></dd>

  <dt>Published at:</dt>
  <dd><%= @press_release.published_at %></dd>

  <dt>Abstract:</dt>
  <dd><%= @press_release.abstract %></dd>

</dl>
